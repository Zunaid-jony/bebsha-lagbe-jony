\">\n      <h3\n        className={\n          props?.products.length === 0 ? \"mt-3\" : \"text-dark ml-2 pb-2\"\n        }\n      >\n          \n      </h3>\n      <div className=\"row productRow\">\n        {props?.products?.map((item, i) => (\n          <div className=\"col-md-3\" key={i} onClick={() => routeHandler(item)}>\n            <Card>\n              {item.condition === \"\" && (\n                <Badge\n                  color=\"success\"\n                  className=\"product__badge text-uppercase\"\n                >\n                   \n                </Badge>\n              )}\n              <CardImg\n                className=\"CardImg\"\n                top\n                width=\"100%\"\n                src={item?.images ? item?.images[0] : noPhoto}\n                alt=\"Card image cap\"\n              />\n              <CardBody className=\"product__cardbody\">\n                <CardTitle> {item?.title}</CardTitle>\n                <CardSubtitle> {item?.price}</CardSubtitle>\n                <CardSubtitle> {item?.location}</CardSubtitle>\n              </CardBody>\n            </Card>\n          </div>\n        ))}\n        {props?.products.length === 0 ? (\n          <div className=\"d-flex align-items-center justify-content-center\">\n            <div className=\"my-5 text-center\">\n              <img\n                src={loadingImage}\n                className=\"img-fluid mx-auto d-block\"\n                alt=\"loading-img\"\n                width=\"50%\"\n              />\n            </div>\n          </div>\n        ) : null}\n      </div>\n    </div>\n  );\n}\n\n\nfunction mapStateToProps(state) {\n  return {\n    products: state.products,\n  };\n}\n\n// function mapDispatchToProps(dispatch) {\n//   return {\n//     getAllProducts: () => dispatch(getAllProducts()),\n//   };\n// }\n\nexport default connect(mapStateToProps, null)(Products);\n","import React from \"react\";\nimport Adds from \"../components/Adds/Adds\";\nimport Appadd